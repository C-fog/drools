{
    "name": "kjl-drools-assist",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.80.0"
    },
    "main": "./dist/extension",
    "activationEvents": [
        "onLanguage:drools"
    ],
    "contributes": {
        "languages": [
            {
                "id": "drools",
                "extensions": [
                    ".drl"
                ],
                "aliases": [
                    "Drools"
                ],
                "configuration": "/language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "drools",
                "path": "/drools.json"
            }
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.4",
        "babel-loader": "^9.2.1",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch"
    },
    "repository": {
    "type": "git",
    "url": "N/A"
  },
  "publisher": "zhangyuge"
}


